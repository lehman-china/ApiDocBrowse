{
  "module": "personal_center",
  "moduleName": "个人中心",
  "explain": "个人中心",
  "apiList": [
    {
      "name": "个人中心首页,信息",
      "url": "/api/personal/home",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {},
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
          "userName": "type=String& explain=用户名称",
          "userAvatarUrl": "type=String& explain=用户头像url",
          "babyName": "type=String& explain=宝宝名称",
          "babyBirthday": "type=String& explain=宝宝生日"
        }
      }
    },
    {
      "name": "收藏,宝典文章/论坛帖子",
      "url": "/api/personal/favorite",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {
        "favoriteType": "type=String& explain=收藏的类型,宝典=FAVORITE_INFORMATION ,贴子=FAVORITE_ARTICLE& default=FAVORITE_ARTICLE",
        "ownerId": "type=String& explain=被收藏数据ID,可以是资讯的ID,也可以是社区话题的ID& default=00014c6fe52ac300",
        "isCancel": "type=String& explain=是否取消收藏,字符串形式的:true/false& default=false"
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
        }
      }
    },
    {
      "name": "医生关注,取消关注",
      "url": "/api/personal/doctorFocus",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {
        "doctorOpenId": "type=String& explain=医生的openId& default=00014c5a7796f11f",
        "isFocus": "type=String& explain=是否关注 (1=关注,0=取消关注)& default=1"
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
        }
      }
    },

    {
      "name": "我的消息列表",
      "url": "/api/personal/myMessageList",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {
        "page": "type=String& explain=页码& default=1",
        "pageSize": "type=String& explain=1页显示的条数,默认16& default=16"
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
          "count": "type=Integer& explain=对话组总数量",
          "lastPage": "type=Boolean& explain=是否是最后一页",
          "contentList": [
            {
              "name": "type=String& explain=对话组名称",
              "avatarUrl": "type=String& explain=对话组显示的头像",
              "content": "type=String& explain=对话组显示的内容",
              "lastTime": "type=String& explain=最后对话时间"
            }
          ]
        }
      }
    },
    {
      "name": "我的收藏列表",
      "url": "/api/personal/myFavoriteList",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {
        "favoriteType": "type=String& explain=查看的类型,宝典=FAVORITE_INFORMATION ,贴子=FAVORITE_ARTICLE& default=FAVORITE_ARTICLE",
        "page": "type=String& explain=页码& default=1",
        "pageSize": "type=String& explain=1页显示的条数,默认16& default=16"
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
          "lastPage": "type=Boolean& explain=是否是最后一页",
          "forumContentList": [
            {
              "forumId": "type=String& explain=帖子ID",
              "forumTypeName": "type=String& explain=类型名称",
              "content": "type=String& explain=内容预览",
              "userName": "type=String& explain=楼主名字",
              "avatarUrl": "type=String& explain=楼主头像url",
              "replyCount": "type=String& explain=回复数统计",
              "praiseCount": "type=String& explain=点赞数统计",
              "showTimeName": "type=String& explain=发帖时间中文显示"
            }
          ]
        }
      }
    },
    {
      "name": "我的贴子列表",
      "url": "/api/personal/myForumList",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {
        "forumType": "type=String& explain=查看的类型,我回复的=reply,我发表的=publish& default=publish",
        "page": "type=String& explain=页码& default=1",
        "pageSize": "type=String& explain=1页显示的条数,默认16& default=16"
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
          "lastPage": "type=Boolean& explain=是否是最后一页",
          "contentList": [
            {
              "forumId": "type=String& explain=帖子ID",
              "forumTypeName": "type=String& explain=类型名称",
              "content": "type=String& explain=内容预览",
              "userName": "type=String& explain=楼主名字",
              "avatarUrl": "type=String& explain=楼主头像url",
              "replyCount": "type=String& explain=回复数统计",
              "praiseCount": "type=String& explain=点赞数统计",
              "showTimeName": "type=String& explain=发帖时间中文显示"
            }
          ]
        }
      }
    },
    {
      "name": "我的名医列表",
      "url": "/api/personal/myDoctorList",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {
        "page": "type=String& explain=页码& default=1",
        "pageSize": "type=String& explain=1页显示的条数,默认16& default=16"
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
          "lastPage": "type=Boolean& explain=是否是最后一页",
          "contentList": [
            {
              "doctorId": "type=String& explain=医生ID",
              "name": "type=String& explain=医生名称",
              "avatarUrl": "type=String& explain=头像Url",
              "focusCount": "type=String& explain=粉丝数量",
              "intro": "type=String& explain=简介",
              "position": "type=String& explain=职称",
              "online": "type=String& explain=是否在线",
              "consultingUrl": "type=String& explain=咨询的url"
            }
          ]
        }
      }
    },
    {
      "name": "我的宝宝列表",
      "url": "/api/personal/myBabyList",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {},
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
          "babyId": "type=String& explain=宝宝ID",
          "name": "type=String& explain=宝宝姓名",
          "relatedId": "type=String& explain=关联标识（父母openId）",
          "gender": "type=String& explain=性别:0为男 1为女",
          "avatarUrl": "type=String& explain=宝宝头像url",
          "birthday": "type=String& explain=宝宝出生日期"
        }
      }
    },
    {
      "name": "添加 与 修改宝宝信息(有说明)",
      "explain": "我的宝宝头像修改步骤: \n  1.把用户上传的图片压缩为100x100像素,控制在200kb以下(接口有判断超过200kb上传错误,或更小减轻服务器负担,);\n  2.把图片数据转换为base64编码\n  3.调用本接口.收工,完毕",
      "url": "/api/personal/saveBaby",
      "type": "POST",
      "contentType": "application/x-www-form-urlencoded;charset=UTF-8",
      "param": {
        "babyId": "type=String& explain=宝宝ID(无是新建,有则是修改)& default=",
        "name": "type=String& explain=宝宝姓名& default=宝宝(接口数据测试)",
        "relatedId": "type=String& explain=关联标识（父母openId）& default=de62ca6c-fa91-4c4f-a01c-325006f3906b",
        "gender": "type=String& explain=性别:0为男 1为女& default=0",
        "birthday": "type=String& explain=宝宝出生日期& default=2015-05-04",
        "avatarImg": "type=String& explain=宝宝头像Base64数据& "
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": {
          "__explain": "系统信息对象"
        }
      }
    },
    {
      "name": "删除我的宝宝信息",
      "url": "/api/personal/deleteBaby",
      "type": "GET",
      "contentType": "text/plain;charset=UTF-8",
      "param": {
        "babyId": "type=String& explain=要删除的宝宝ID& default=21de8b2a-31eb-4bfe-b62b-05bb62cd0474"
      },
      "result": {
        "state": "type=String& explain=状态码,0表示成功",
        "message": "type=String& explain=返回消息",
        "data": null
      }
    }
  ]
}